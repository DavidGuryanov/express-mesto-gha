{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off"
  },
  "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
}
